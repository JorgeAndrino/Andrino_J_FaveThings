<section id="things">
    <!-- this is a loop with handlebars -->
    {{#each image}}
        <div class="container">
            <a href="/{{title}}">
                <img src="/images/{{image}}" alt="image">
            </a>

            <h3 class="title">{{title}}</h3>
            <h4>{{heading}}</h4>
        </div>        
    {{/each}}
</section>
